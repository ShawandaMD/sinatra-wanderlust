<h1>Hello <%=@user.name %></h1>
<% if flash.has?(:notice) %>
<%= flash[:notice] %>
<%end%>

<h2><a href="/journal_entry/new">New Journal Entry</a></h2>

<ul>
  <%@entries_all.each do |entry| %>
  <li>
    <a href="/journal_entries/<%=entry.id %>">
      <%=entry.title%> - <%=entry.country_name%>
    </a>
  </li>
  <%end %>
</ul>

<h1></h1><a href="/logout">Log out</a> </h1>
